{
  "name": "shipengine-integration-platform-local-dev-api",
  "version": "0.0.1",
  "description": "The ShipEngine Integration Platform local development API",
  "author": {
    "name": "ShipEngine",
    "email": "support@shipengine.com",
    "url": "https://www.shipengine.com"
  },
  "license": "Apache-2.0",
  "homepage": "https://www.shipengine.com/docs/integration-platform/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ShipEngine/shipengine-integration-platform-local-dev-api.git"
  },
  "bugs": {
    "url": "https://github.com/ShipEngine/shipengine-integration-platform-local-dev-api/issues"
  },
  "main": "lib/server.js",
  "scripts": {
    "build": "tsc",
    "bump": "bump --tag --push --all",
    "clean": "shx rm -rf coverage lib",
    "coverage": "nyc npm run test",
    "format": "prettier --write src/**/*.ts",
    "lint": "eslint src/**/*.ts",
    "release": "npm run upgrade && npm run clean && CI=true react-scripts test && npm run bump",
    "start": "node lib/server.js 4001",
    "test": "mocha",
    "upgrade": "npm-check -u && npm audit fix"
  },
  "dependencies": {
    "@shipengine/integration-platform-loader": "0.0.35",
    "cors": "^2.8.5",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@jsdevtools/version-bump-prompt": "^6.0.6",
    "@types/cors": "^2.8.7",
    "@types/eslint": "^7.2.0",
    "@types/express": "^4.17.7",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "mocha": "^8.1.1",
    "nodemon": "^2.0.4",
    "npm-check": "^5.9.2",
    "shx": "^0.3.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=10"
  },
  "engineStrict": true
}
